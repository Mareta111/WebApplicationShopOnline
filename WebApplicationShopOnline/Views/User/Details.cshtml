@model WebApplicationShopOnline.Models.User1

@{
    ViewData["Title"] = "User Details";
}

<h2>User Details</h2>

<div>
    <dl class="row">
        <dt class="col-sm-2">ID:</dt>
        <dd class="col-sm-10">@Model.IdUser</dd>

        <dt class="col-sm-2">Name:</dt>
        <dd class="col-sm-10">@Model.Name</dd>

        <dt class="col-sm-2">Login:</dt>
        <dd class="col-sm-10">@Model.Login</dd>

        <dt class="col-sm-2">Password:</dt>
        <dd class="col-sm-10">@Model.Password</dd>

        <dt class="col-sm-2">Telephone:</dt>
        <dd class="col-sm-10">@Model.Telephone</dd>

        <dt class="col-sm-2">Email:</dt>
        <dd class="col-sm-10">@Model.Email</dd>
    </dl>
</div>

<a asp-action="Index" class="btn btn-primary">Back to List</a>